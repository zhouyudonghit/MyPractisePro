<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.localuser.retrofittest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Toolbar.ToolbarActivity" android:theme="@style/APPNoActionBarTheme"></activity>

        <activity android:name=".DrawLayout.DrawLayoutActivity" android:theme="@style/APPNoActionBarTheme"></activity>

        <activity android:name=".Canvas.CanvasActivity"></activity>

        <activity android:name=".View.MyViewActivity"></activity>

        <activity android:name=".AnimatorTest.AnimatorTestActivity"></activity>
        
        <activity android:name=".PullRefreshListView.PullRefreshActivity"></activity>

        <activity android:name=".MotionEventTest.MotionEventTestActivity"></activity>
    </application>

</manifest>